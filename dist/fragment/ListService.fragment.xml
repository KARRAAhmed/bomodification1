<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		id="myDialog"
		noDataText="Pas de service"
		title="Choisir un service"
		search=".handleSearch"
		confirm=".handleClose"
		cancel=".handleClose"
		items="{
			path : '/ServicesSet'
		}">
		<ColumnListItem>
			<cells>
				<Text text="{Zlibserv}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column >
				<header>
					<Text text="Service" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>